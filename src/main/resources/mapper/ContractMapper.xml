<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.wyx.isisystem.dao.ContractDao">

    <insert id="insertContract" parameterType="com.wyx.isisystem.entity.Contract">
        <selectKey resultType="java.lang.Integer" order="AFTER" keyProperty="id">
            SELECT LAST_INSERT_ID()
        </selectKey>
        INSERT INTO client_contract_info(client_id, project_id, check_system_id)
        VALUE (#{clientId}, #{projectId}, #{checkSystemId})
    </insert>

</mapper>